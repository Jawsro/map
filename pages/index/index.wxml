<!--首页-->
<!-- 组件引入 -->
<!-- <header  my-property="施工地图" ></header> -->
<view class="container">
  <view class="status">
    <view class="status-item" wx:for="{{statusList}}" wx:key='index' bindtap="select" data-index="{{index}}">
      <view class="item {{item.checked?'select-active':'noselect'}}">
        <text class="iconfont {{item.icon}} margin-right" 
        style="color:{{item.checked?item.color:'#606262'}};font-size:40rpx"></text>
        <text class="font-14">{{item.text}}</text>
      </view>
    </view>
  </view>
  <view class="search-box">
   <view class="search">
     <input placeholder="请输入搜索的内容" class="search-input font-14"></input>
     <text class="iconfont icon-shouqi1 icon " style="font-size:40rpx;"></text>
   </view>
  </view>
  <!-- <view class="section">
    <picker bindchange="bindPickerChange" value="{{index}}" range="{{array}}">
      <view class="picker">
        当前选择：{{array[index]}}
      </view>
    </picker>
  </view> -->
<!-- longitude='111.286331' 
    latitude='30.69178'  -->
  <map id="map"  scale="{{scale}}" 
    longitude='{{longitude}}' 
    latitude='{{latitude}}' 
    markers="{{markers}}"   
    bindmarkertap="markertap" 
    show-location 
    style="width: 100%; height: 100%;"
  ></map>

  <view class="msg-box" hidden="{{msgBoxIsShow}}">
    <view class="box-top">
       <view class="shouqi" hidden="{{zhedie}}" bindtap="openMsgBox">
        <text class="iconfont icon-shouqi" style="font-size:60rpx;color:#CDCECE"></text>
      </view>
      <view class="shouqi" hidden="{{!zhedie}}" bindtap="closeMsgBox">
        <text class="iconfont icon-shouqi1" style="font-size:60rpx;color:#CDCECE"></text>
      </view>
      <view class="project">
        <view class="project-title">
          <view class="title">
            <text class="font-18 foont-weight-600 margin-right-10">楠海花园6-2</text>
            <text class="font-12 project-status">施工完成</text>
          </view>
          <view class="close">
            <text 
              class="iconfont icon-ai54" 
              style="font-size:60rpx;color:#939392" 
              hidden="{{zhedie}}"
              bindtap="MsgBoxNoShow">
            </text>
            <view class="daohang" hidden="{{!zhedie}}" bindtap="getDaoHang">
              <text class="iconfont icon-daohang1 margin-right-10" style="font-size:44rpx;color:#3A66A8;" ></text>
              <text class="font-14">一键导航</text>
            </view>
          </view>
        </view>
        <view class="padding-top font-14">
          <text style="padding-right:10rpx;border-right:1px solid #939392">联审号：2020-00</text>
          <text style="padding-left:10rpx">联审日期：2020.1.49</text>
        </view>
        <view class="font-14 padding-top">
          <text >项目地址：高新区楠海花园小区</text>
        </view>
        <view class="daohang" hidden="{{zhedie}}" bindtap="getDaoHang">
          <text class="iconfont icon-daohang1 margin-right-10" style="font-size:44rpx;color:#3A66A8;"></text>
          <text class="font-14">一键导航</text>
        </view>
      </view>
    </view>
    <view class="project-details" hidden="{{!zhedie}}">
      <view class="project-details-item">
        <view class="font-18 foont-weight-600">
          <text>负责人</text>
        </view>
        <view class="person all-style">
          <text class="margin-right-10">孟凡清</text>
          <text class="phone">13872675891</text>
        </view>
        <view class="classification">
          <view class="classification-item">
            <view class="font-18 foont-weight-600">品牌：</view>
            <view class="font-14 margin-top-10">
            通力电梯</view>
          </view>
          <view class="classification-item ">
            <view class="font-18 foont-weight-600">品牌：</view>
            <view class="font-14 margin-top-10">
            通力电梯</view>
          </view>
          <view class="classification-item ">
            <view class="font-18 foont-weight-600">品牌：</view>
            <view class="font-14 margin-top-10">
            通力电梯</view>
          </view>
          <view class="classification-item">
            <view class="font-18 foont-weight-600">品牌：</view>
            <view class="font-14 margin-top-10">
            通力电梯</view>
          </view>
        </view>
      </view>

      <view class="project-details-item">
        <view class="font-18 foont-weight-600">
          <text>监理</text>
        </view>
        <view class="person all-style">
          <text class="">宜昌市平湖工程建设监理</text>
        </view>
      </view>

      <view class="project-details-item">
        <view class="font-18 foont-weight-600">
          <text>底坑基础（自拌/商砼）</text>
        </view>
        <view class="person all-style">
          <text class="">商品砼</text>
        </view>
      </view>

      <view class="project-details-item">
        <view class="font-18 foont-weight-600">
          <text>井道连廊（现场制作/工厂制作）</text>
        </view>
        <view class="person all-style">
          <text class="">工厂制作</text>
        </view>
      </view>
    </view>
  </view>
</view>
